    "doc": "jsdoc --verbose -c jsdoc.conf --readme README.md --package package.json src/index.js -d doc",
    "prepublish": "gulp"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/emersion/mpris-service.git"
  },
  "keywords": [
    "mpris",
