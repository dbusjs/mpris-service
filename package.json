{
  "name": "mpris-service",
  "version": "2.1.0",
  "description": "Node.js implementation for the MPRIS D-Bus Interface Specification to create a mediaplayer service",
  "main": "dist/index.js",
  "scripts": {
    "test": "gulp && dbus-run-session -- jest",
    "coverage": "gulp && dbus-run-session -- jest --coverage",
